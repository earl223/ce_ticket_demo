{
	"manifest_version": 2,
	"name": "Ticket Manager",
	"version": "1.0",
	"description": "This extension monitors the queue for re-opens.",
	"icons": {
		"128": "icon128.png",
		"48": "icon48.png",
		"16": "icon16.png"
	},
	"browser_action": {
			"default_icon": "icon16.png",
			 "default_popup": "popup.html"
		 },
	 "permissions":[
	 	"notifications"
	 ], 
 	"background": {
	    "scripts": ["background.js"],
	    "persistent": false
  	}
}